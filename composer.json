{
    "name": "huang-yi/laravel-db-log",
    "description": "Logs database queries for Laravel.",
    "keywords": ["laravel", "database", "sql", "log"],
    "license": "MIT",
    "authors": [
        {
            "name": "Huang Yi",
            "email": "coodeer@163.com"
        }
    ],
    "require": {
        "illuminate/database": "~5.6",
        "illuminate/http": "~5.6",
        "illuminate/log": "~5.6",
        "illuminate/support": "~5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "HuangYi\\DBLog\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HuangYi\\DBLog\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HuangYi\\DBLog\\ServiceProvider"
            ]
        }
    }
}
